<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>반목문 : while</title>
<script type="text/javascript">
//0-10까지
	var i = 0;
	while (i<11) {
		document.write(i+"&nbsp;&nbsp;");
		i++
	}

	var l = 0;
	while (l<15) {
		document.write(i+"&nbsp;&nbsp;");
		i++
	}
	
//5단
	var k = 1;
	while (k<10) {
		document.write("5 *"+k+"="+(5*k)+"<br>");
		k++;
	}
	document.write("<hr>");
	
	// 구구단
	 i = 2;
	 while(i<10){
		 var j= 1;
	 	 while(j<10){
	 		 document.write(i+ " * " + j + " = " + (i*j) + "<br>"); 
		   j++;	 
		 }
		i++;
	 }


</script>
</head>
